# Backend environment variables
# PUBLIC: Do not store secrets in VCS. Copy this file to .env and fill in values.

# PostgreSQL connection string used by backend
# Example aligned with local notes_database container:
# postgresql://appuser:dbuser123@localhost:5000/myapp
DATABASE_URL=postgresql://<user>:<password>@<host>:<port>/<database>

# Allowed frontend origin for CORS
FRONTEND_ORIGIN=http://localhost:3000
